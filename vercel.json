{

    "builds": [{
        "src": "LinkHouse/wsgi.py",
        "use": "@vercel/python"

    },
    {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": { 
          "distDir": "static" 
        }
      }
],

    "routes": [

        {

            "src": "/(.*)",

            "dest": "LinkHouse/wsgi.py"

        }

    ]

}